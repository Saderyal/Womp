{
	"compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": true,
		"module": "ES6",
    "lib": ["es2020","dom"],
		"target": "ES2021",
		"jsx": "react-jsx",
		"jsxImportSource": "../",
    "downlevelIteration": true,
		"moduleResolution": "node",
    "skipLibCheck": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationDir": ".",
	},
  "include": ["ts/womp.ts"],
  "exclude": ["dist", "./jsx-runtime.ts"]
}
